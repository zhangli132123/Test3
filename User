/*
用户实体
*/

public class User {
    private String id;             //编号
    private String password;      //密码
    void setID(String id) {
        this.id=id;
    }
    void setPassword(String password) {
        this.password=password;
    }

    String getID() {
        return this.id;
    }
    String getPassword() {
        return this.password;
    }

}
